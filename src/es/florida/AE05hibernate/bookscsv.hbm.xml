<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="es.florida.AE05hibernate">
<class name="BooksCSV" table="BooksCSV">
	<id name="id" column="id">
	<generator class="increment"/>
	</id>
	<property name="Titulo" type="string" column="Titulo"/>
	<property name="Autor" type="string" column="Autor"/>
	<property name="Anyo_nacimiento" type="string" column="Anyo_nacimiento"/>
	<property name="Anyo_publicacion" type="string" column="Anyo_publicacion"/>
	<property name="Editorial" type="string" column="Editorial"/>
	<property name="Numero_de_paginas" type="string" column="Numero_de_paginas"/>
</class>
</hibernate-mapping>